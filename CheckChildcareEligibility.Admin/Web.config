<configuration>
    <system.webServer>
        <security>
            <requestFiltering removeServerHeader="true">
                <requestLimits maxAllowedContentLength="104857600" />
            </requestFiltering>
            
        </security>
        <httpProtocol>
            <customHeaders>
                <remove name="X-Powered-By"/>
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
